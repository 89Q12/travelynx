<div class="row">
	<div class="col s12">
		<div class="card caution-color">
			<div class="card-content white-text">
				<span class="card-title">404 Not Found</span>
				% if (my $m = stash('message')) {
					<p><%= $m %></p>
				% }
				% else {
					<p>Diese Seite gibt es hier nicht.</p>
				% }
			</div>
		</div>
	</div>
</div>
