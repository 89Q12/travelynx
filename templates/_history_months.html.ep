<div class="row">
	<div class="col s12">
		<ul class="pagination">
			% for my $month (journeys->get_months(uid => current_user->{id})) {
				% my $link_to = $month->[0];
				% my $text = $month->[1];
				% my $class = $link_to eq $current ? 'active' : 'waves-effect';
				<li class="<%= $class %>"><a href="/history/<%= $link_to %>"><%= $text %></a></li>
			% }
		</ul>
	</div>
</div>
