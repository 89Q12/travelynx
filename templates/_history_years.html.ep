<div class="row">
	<div class="col s12">
		<ul class="pagination">
			% for my $year (journeys->get_years(uid => current_user->{id})) {
				% my $link_to = $year->[0];
				% my $text = $year->[1];
				% my $class = $link_to eq $current ? 'active' : 'waves-effect';
				<li class="<%= $class %>"><a href="/history/<%= $link_to %>"><%= $text %></a></li>
			% }
		</ul>
	</div>
</div>
